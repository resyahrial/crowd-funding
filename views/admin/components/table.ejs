<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Business Value</th>
      <th scope="col">Fund Raised</th>
      <th scope="col">Remain</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% data.forEach(elem => { %>
      <% if (elem.business_value > getFundRaised(elem.UserFunds)) { %>
        <tr>
          <th scope="row"><%= elem.name %></th>
          <td><%= parseCurrency(elem.business_value) %></td>
          <td><%= parseCurrency(getFundRaised(elem.UserFunds)) %></td>
          <td><%= elem.getRemainDate() %> day</td>
          <td>
            <a href="<%= `/admin/funds/edit/${elem.id}` %> " class="btn btn-warning">Edit</a>
            <a href="<%= `/admin/funds/delete/${elem.id}` %> " class="btn btn-danger">Delete</a>
            <a href="<%= `/admin/funds/${elem.id}/users` %> " class="btn btn-primary">Show Investor</a>
          </td>
        </tr>
      <% } %>
    <% }) %>
  </tbody>
</table>