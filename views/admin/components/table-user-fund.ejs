<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Amount</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>
    <% data.forEach(elem => { %>
      <tr>
        <th scope="row"><%= elem.name %></th>
        <td><%= parseCurrency(elem.amount) %></td>
        <td><%= elem.status %></td>
      </tr>
    <% }) %>
  </tbody>
</table>